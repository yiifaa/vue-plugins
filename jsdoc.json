{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },

  "source": {
    "include": ["src/"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },

  "plugins": [],

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },

  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "docs/api",
    "recurse": true,
    "tutorials": "docs/tutorials"
  }
}
